<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Activity.ChallengeTotalPhotoActivity">
    <LinearLayout android:layout_width="match_parent" android:layout_height="48dp">
        <ImageView
                android:src="@drawable/ic_backarrow"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="14dp"
                android:layout_width="24dp" android:layout_height="24dp"/>
        <TextView
                android:text="전체 인증 사진"
                android:textColor="#53565b"
                android:textSize="18sp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="12dp"
                android:layout_width="wrap_content" android:layout_height="wrap_content"/>
    </LinearLayout>
    <ImageView
            android:layout_marginTop="48dp" android:layout_width="match_parent" android:layout_height="4dp" android:background="@drawable/border_search_toolbar"/>
</RelativeLayout>